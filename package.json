{
    "name": "@tsip/flow-react",
    "version": "0.2.0",
    "description": "React hooks and utilities for integrating TSIP reactive data flows into React applications",
    "keywords": [
        "react",
        "hooks",
        "reactive",
        "flow",
        "tsip",
        "typescript",
        "suspense",
        "ssr",
        "hydration",
        "async",
        "state-management",
        "effects"
    ],
    "author": "Maksim Zemskov <maxaz74@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/nodge/tsip-flow-react/issues"
    },
    "homepage": "https://github.com/nodge/tsip-flow-react",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nodge/tsip-flow-react.git"
    },
    "engines": {
        "node": ">=18"
    },
    "packageManager": "pnpm@10.18.2",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "default": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        },
        "./server": {
            "import": {
                "types": "./dist/server.d.mts",
                "default": "./dist/server.mjs"
            },
            "default": {
                "types": "./dist/server.d.ts",
                "default": "./dist/server.js"
            }
        }
    },
    "files": [
        "README.md",
        "CHANGELOG.md",
        "LICENSE",
        "dist"
    ],
    "sideEffects": false,
    "scripts": {
        "build": "tsup",
        "lint": "eslint .",
        "fmt": "prettier --write . --log-level error",
        "test": "vitest",
        "typecheck": "tsc --noEmit",
        "changeset": "changeset add",
        "ci:release": "pnpm run build && pnpm exec changeset publish"
    },
    "dependencies": {
        "serialize-javascript": "^6.0.2"
    },
    "devDependencies": {
        "@changesets/changelog-github": "0.5.1",
        "@changesets/cli": "2.29.7",
        "@eslint/js": "9.36.0",
        "@testing-library/dom": "10.4.1",
        "@testing-library/jest-dom": "6.8.0",
        "@testing-library/react": "16.3.0",
        "@tsip/flow": "0.2.0",
        "@tsip/types": "0.4.0",
        "@types/eslint": "9.6.1",
        "@types/react": "19.2.2",
        "@types/react-dom": "19.2.1",
        "@types/serialize-javascript": "5.0.4",
        "@vitest/coverage-v8": "3.2.4",
        "eslint": "9.36.0",
        "eslint-plugin-react-hooks": "7.0.0",
        "jsdom": "27.0.0",
        "lefthook": "1.13.4",
        "prettier": "3.6.2",
        "react": "19.2.0",
        "react-dom": "19.2.0",
        "tsup": "8.5.0",
        "typescript": "5.9.2",
        "typescript-eslint": "8.44.1",
        "vitest": "3.2.4"
    },
    "peerDependencies": {
        "react": ">=18.0.0"
    }
}
